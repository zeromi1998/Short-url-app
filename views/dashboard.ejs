<div class="container">
    
  <h1 class="text-center ">Shorten URL </h1>
  <form action="/dashboard" method="POST" class="my-4 form-inline">
     
      <input placeholder="Enter your Url" type="url" name="fullUrl" required class="form-control m-2 w-75">
      <button type="submit" class="btn btn-success">Shorten Link</button>
  </form>




 

</div>


<table class="table table-striped table-responsive m-2">
  <thead>
      <tr>
          <th>Full URL</th>
          <th>Short URL</th>
      </tr>

  </thead>
  <tbody>
    <% sUrl.forEach(url => { %>
      <tr>
          <td>
              <a href="<%= url.full %> " ><%= url.full %></a>
          </td>
          <td>
              <a href="<%= url.short %> " target="_blank"><%= url.short %> </a>
          </td>
      </tr>
    <%   });%>
  </tbody>

</table>
    </div>